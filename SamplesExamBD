Odpowiedzi do przykładowego testu Bazy danych 2
SELECt Książki.Id_książki, Książki.Tytuł
FROM Książki
WHERE Książki.Tytuł LIKE N'P%' AND Książki.Język_oryginału = 'polski'

SELECT DISTINCT Książki.Tytuł, Autorzy.Nazwisko, Autorzy.Imie1
FROM Autorzy JOIN Książki
ON Autorzy.Id_autora = Książki.Id_książki
WHERE Autorzy.Data_urodzenia > '1980-01-01'

SELECT Autorzy.Id_autora,Nazwisko, Imie1
FROM Autorzy LEFT JOIN  Książki
ON Książki.Id_Książki = Autorzy.Id_autora
WHERE Książki.Id_książki IS NULL

SELECT Książki.Id_książki, AVG(Egzemplarze.Cena) Sredniacena
FROM Książki JOIN Egzemplarze
ON Książki.Id_książki = Egzemplarze.Id_Ksiazki
WHERE Egzemplarze.Stan = 4
group by Książki.Id_książki


SELECT Autorzy.Id_autora, COUNT(Książki.Tytuł) LiczbaTytulow
FROM Autorzy, Książki
WHERE Autorzy.Id_autora = Książki.Id_książki
GROUP BY Autorzy.Id_autora
HAVING COUNT(Książki.Tytuł) = 1
